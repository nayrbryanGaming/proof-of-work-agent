{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@pow_agent_api_url",
    "NEXT_PUBLIC_WS_URL": "@pow_agent_ws_url"
  }
}
